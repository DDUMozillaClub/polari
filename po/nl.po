#. commands that would be nice to support: */
#.
#. AWAY: N_("/AWAY [<message>] - sets or unsets away message"),
#. LIST: N_("/LIST [<channel>] - lists stats on <channel>, or all channels on the server"),
#. MODE: "/MODE <mode> <nick|channel> - ",
#. NOTICE: N_("/NOTICE <nick|channel> <message> - sends notice to <nick|channel>"),
#. OP: N_("/OP <nick> - gives channel operator status to <nick>"),
#. WHOIS: N_("/WHOIS <nick> - requests information on <nick>"),
#. */
#: ../src/ircParser.js:26
msgid ""
"/HELP [<command>] - displays help for <command>, or a list of available "
"commands"
msgstr ""
"/HELP [<opdracht>] - toont hulp voor <opdracht>, of een lijst met "
"beschikbare opdrachten"

#: ../src/ircParser.js:27
msgid ""
"/INVITE <nick> [<channel>] - invites <nick> to <channel>, or the current one"
msgstr ""
"/INVITE <bijnaam> [<kanaal>] - nodigt <bijnaam> uit voor <kanaal>, of voor "
"het huidige kanaal"

#: ../src/ircParser.js:28
msgid "/JOIN <channel> - joins <channel>"
msgstr "/JOIN <kanaal> - <kanaal> binnengaan"

#: ../src/ircParser.js:29
msgid "/KICK <nick> - kicks <nick> from current channel"
msgstr "/KICK <bijnaam> - <bijnaam> uit het huidige kanaal verwijderen"

#: ../src/ircParser.js:30
msgid "/ME <action> - sends <action> to the current channel"
msgstr "/ME <actie> - <actie> naar het huidige kanaal sturen"

#: ../src/ircParser.js:31
msgid "/NAMES - lists users on the current channel"
msgstr "/NAMES - gebruikers op het huidige kanaal tonen"

#: ../src/ircParser.js:32
msgid "/NICK <nickname> - sets your nick to <nickname>"
msgstr "/NICK <bijnaam> - maakt uw bijnaam gelijk aan <bijnaam>"

#: ../src/ircParser.js:33
msgid ""
"/PART [<channel>] [<reason>] - leaves <channel>, by default the current one"
msgstr ""
"/PART [<kanaal>] [<reden>] - <kanaal> verlaten, standaard het huidige kanaal "

#: ../src/ircParser.js:34
msgid "/QUERY <nick> - opens a private conversation with <nick>"
msgstr "/QUERY <bijnaam> - een privégesprek met <bijnaam> beginnen"

#: ../src/ircParser.js:35
msgid "/QUIT [<reason>] - disconnects from the current server"
msgstr "/QUIT [<reden>] - verbinding met huidige server verbreken"

#: ../src/ircParser.js:36
msgid "/SAY <text> - sends <text> to the current room/contact"
msgstr "/SAY <tekst> - <tekst> naar de huidige ruimte/contact sturen"

#: ../src/ircParser.js:37
msgid "/TOPIC <topic> - sets the topic to <topic>, or shows the current one"
msgstr ""
"/TOPIC <onderwerp> - onderwerp instellen op <onderwerp>, of het huidige tonen"

#: ../src/ircParser.js:40
msgid "Unknown command - try /HELP for a list of available commands"
msgstr ""
"Onbekende opdracht - gebruik /HELP voor een lijst met beschikbare opdrachten"

